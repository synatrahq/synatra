{
  "name": "@synatra/code-executor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "node --import tsx --watch src/index.ts",
    "build": "bun build src/index.ts --target=node --outfile=dist/index.js --external isolated-vm",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "1.19.6",
    "@hono/zod-validator": "^0.7.5",
    "@synatra/core": "workspace:*",
    "@synatra/service-call": "workspace:*",
    "@synatra/util": "workspace:*",
    "hono": "4.11.4",
    "isolated-vm": "^6.0.2",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "tsx": "4.19.4",
    "typescript": "5.9.3",
    "vitest": "^4.0.15"
  }
}
