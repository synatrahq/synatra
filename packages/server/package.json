    "@octokit/auth-app": "^7.1.1",
    "@synatra/core": "workspace:*",
    "@synatra/service-call": "workspace:*",
    "@synatra/util": "workspace:*",
    "@synatra/workflows": "workspace:*",
    "@temporalio/client": "1.13.2",
    "ai": "^5.0.105",
    "ajv": "8.17.1",
    "better-auth": "1.4.5",
    "drizzle-orm": "0.43.0",
